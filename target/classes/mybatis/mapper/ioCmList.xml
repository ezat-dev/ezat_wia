<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="ioCmList">
	<select id="getIoCmList" resultType="ioCmList" parameterType="ioCmList">
		SELECT *
		FROM tb_monitor_info_cm
		WHERE 1=1
		AND mi_webdir = #{dbGroup}
	</select>
	
</mapper>