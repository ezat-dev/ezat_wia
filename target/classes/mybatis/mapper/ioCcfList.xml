<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="ioCcfList">
	<select id="getIoCcfList" resultType="ioCcfList" parameterType="ioCcfList">
		SELECT *
		FROM tb_monitor_info_ccf
		WHERE 1=1
		AND mi_webdir = #{dbGroup}
	</select>
	
</mapper>